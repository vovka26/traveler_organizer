<h1>Categories</h1>
  <div class="container">
  <h2>Check out all the places you can visit!</h2>
  </div>
        <% @categories.each do |category|%>
    <div class="list-group-item">
      <%= link_to category.name, category_path(category.id) %>
    </div>
      <% end %>
      <% @categories.each do |category| %>
      <% if category.image.attached? %>
    <p><a href="<%=category_path(category)%>" ><%= image_tag(category.image, :size => "304x236") %></a></p>
       <% end %>
       <% end %>
<div> <%= link_to "New Category", new_category_path %>
<div> <%= link_to "Main Page", welcome_index_path %> </div>
